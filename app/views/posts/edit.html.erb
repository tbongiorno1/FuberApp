<div class="container">
  <div class="row">
    <div class="col-lg-3">
      <div class="page-header">
        <h2>Settings</h2>
      </div>
      <%= render partial: "admins/partials/sidenav" %>
    </div>
    <div class="col-lg-9">
      <div class="page-header">
        <h2>Edit Post</h2>
      </div>
      <div class="panel panel-default">
        <div class="panel-body">
          <div class="form-group">
            <%= render 'form' %>
          </div>
        </div>
      </div>
      <div class="form-group text-center">
        <ul class="list-inline">
          <li><%= link_to 'Show', course_post_path(@course) %></li>
          <li><%= link_to 'Back', course_path(@course) %></li>
        </ul>
      </div>
    </div>
  </div>
</div>
