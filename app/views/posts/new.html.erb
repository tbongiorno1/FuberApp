<div class="container">
  <div class="row">
    <div class="col-lg-3">
      <div class="page-header">
        <h2>Settings</h2>
      </div>
      <%= render partial: "admins/partials/sidenav" %>
    </div>
    <div class="col-lg-9">
      <div class="row">
        <div class="col-lg-12">
          <div class="page-header">
            <h2>New Post</h2>
          </div>
          <div class="panel panel-default">
            <div class="panel-body">
              <div class="form-group">
                <%= render 'form' %>
              </div>
              <div class="form-group text-center">
                <%= link_to 'Back', course_path(@course) %>
              </div>
            </div>
          </div>
        </div>
      </div>
    </div>
  </div>
</div>
